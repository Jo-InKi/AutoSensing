<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="CodeMapper">
	<cache />
	
	<select id="selectCodeListByClass" resultType="com.auto.sensing.vo.CodeVO">
		SELECT
			class AS classification
			, code
			, name
			, etc
		FROM code_tb
		WHERE class = #{classification}
	</select>
</mapper>