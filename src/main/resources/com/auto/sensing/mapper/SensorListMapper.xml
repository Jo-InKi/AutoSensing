<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="SensorListMapper">
	<cache />

	<select id="selectSensorListByCompany" resultType="com.auto.sensing.vo.SensorListVO">
		SELECT
			code
			, sensorcompany
			, sensorkind
			, name
			, calcvalue
			, etc
		FROM sensorlist_tb
		WHERE sensorcompany = #{sensorcompany}
	</select>
	
</mapper>